<%= simple_form_for [@game] do |f| %>
  <div class="form-group">
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :category %>
    <%= f.input :console %>
    <%= f.input :photo, as: :file %>
  </div>
  <%= f.submit class: 'btn btn-primary' %>
<% end %>
